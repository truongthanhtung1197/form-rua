{
  "name": "landing-page",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit && tsc --project admin/tsconfig.json --pretty --noEmit",
    "deploy": "yarn lint && vercel deploy --prod"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/line-clamp": "0.3.1",
    "apisauce": "2.1.5",
    "cookies-next": "2.0.3",
    "formik": "2.2.9",
    "lodash.debounce": "4.0.8",
    "masonry-layout": "4.2.2",
    "moment": "2.29.1",
    "next": "12.0.4",
    "nprogress": "0.2.0",
    "ramda": "0.27.1",
    "ramda-adjunct": "2.35.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-masonry-css": "1.0.16",
    "react-multi-carousel": "2.6.5",
    "react-player": "2.9.0",
    "react-quill": "1.3.5",
    "sass": "1.43.5",
    "sweetalert2": "11.3.3",
    "swr": "1.1.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@types/node": "16.11.10",
    "@types/nprogress": "0.2.0",
    "@types/ramda": "0.27.66",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-slick": "0.23.7",
    "autoprefixer": "10.4.0",
    "eslint": "7",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-sort-imports-es6-autofix": "0.6.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "postcss": "8.4.5",
    "prettier": "2.5.0",
    "tailwindcss": "3.0.5",
    "typescript": "4.5.2"
  },
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "google",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "@typescript-eslint"
  ],
  "rules": {
    "require-jsdoc": "off",
    "react/react-in-jsx-scope": "off"
  }
}
